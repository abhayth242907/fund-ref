# Backend API Configuration
# URL of the backend API server
REACT_APP_API_BASE_URL=http://localhost:8000

# Application Configuration
REACT_APP_NAME=Fund Referential System
REACT_APP_VERSION=1.0.0

# Pagination Defaults
# Default number of items per page
REACT_APP_DEFAULT_PAGE_SIZE=10
# Maximum number of items per page
REACT_APP_MAX_PAGE_SIZE=100

# Feature Flags
# Enable/disable fund hierarchy visualization
REACT_APP_ENABLE_VISUALIZATION=true
# Enable/disable fund onboarding form
REACT_APP_ENABLE_ONBOARDING=true
# Enable/disable analytics dashboard
REACT_APP_ENABLE_ANALYTICS=true
